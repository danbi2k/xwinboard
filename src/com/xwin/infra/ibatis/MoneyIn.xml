<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">
	
<sqlMap namespace="MoneyIn">

	<typeAlias alias="MoneyIn" type="com.xwin.domain.user.MoneyIn" />
	
	<resultMap id="moneyInResultMap" class="MoneyIn">
		<result property="id" column="ID"/>
		<result property="userId" column="USERID"/>
		<result property="nickName" column="NICKNAME"/>
		<result property="money" column="MONEY"/>
		<result property="status" column="STATUS"/>
		<result property="reqDate" column="REQ_DATE"/>
		<result property="procDate" column="PROC_DATE"/>
		<result property="name" column="NAME"/>
		<result property="bankBookId" column="BANKBOOK_ID"/>
		<result property="bankBook" column="BANKBOOK_ID" select="selectBankBook"/>
		<result property="isChecked" column="ISCHECKED"/>
		<result property="isDeleted" column="ISDELETED"/>
		<result property="note" column="NOTE"/>
		<result property="isRemoved" column="ISREMOVED"/>
	</resultMap>
	
	<insert id="insertMoneyIn" parameterClass="MoneyIn">
		INSERT INTO tbl_money_out(USERID, MONEY, STATUS, REQ_DATE, NAME, BANKBOOK_ID, PROC_DATE, NICKNAME, NOTE, TYPE, SOURCE)
		VALUES(#userId#, #money#, #status#, #reqDate#, #name#, #bankBookId#, #procDate#, #nickName#, #note#, 'in', #source#)
	</insert>
	
	<update id="updateMoneyIn" parameterClass="MoneyIn">
		UPDATE tbl_money_out
		<dynamic prepend="SET">
			<isNotNull prepend="," property="userId">
			USERID = #userId#
			</isNotNull>
			<isNotNull prepend="," property="name">
			NAME = #name#
			</isNotNull>
			<isNotNull prepend="," property="nickName">
			NICKNAME = #nickName#
			</isNotNull>			
			<isNotNull prepend="," property="status">
			STATUS = #status#
			</isNotNull>
			<isNotNull prepend="," property="procDate">
			PROC_DATE = #procDate#
			</isNotNull>
			<isNotNull prepend="," property="isDeleted">
			ISDELETED = #isDeleted#
			</isNotNull>
			<isNotNull prepend="," property="isChecked">
			ISCHECKED = #isChecked#
			</isNotNull>
			<isNotNull prepend="," property="isRemoved">
			ISREMOVED = #isRemoved#
			</isNotNull>
		</dynamic>
		WHERE ID = #id#
	</update>
	
	<update id="deleteMoneyIn" parameterClass="string">
		UPDATE tbl_money_out SET ISDELETED = 'Y' WHERE ID = #value#
	</update>
	
	<select id="selectMoneyIn" resultMap="moneyInResultMap" parameterClass="string">
		SELECT * FROM tbl_money_out WHERE ID = #value#
	</select>
	
	<select id="selectMoneyInList" resultMap="moneyInResultMap" parameterClass="map">
		SELECT * FROM tbl_money_out
		WHERE TYPE = 'in'
			<isNotNull prepend="AND" property="userId">
				USERID = #userId#
			</isNotNull>
			<isNotNull prepend="AND" property="userIdLike">
				USERID LIKE #userIdLike#
			</isNotNull>
			<isNotNull prepend="AND" property="nickNameLike">
				NICKNAME LIKE #nickNameLike#
			</isNotNull>
			<isNotNull prepend="AND" property="status">
				STATUS = #status#
			</isNotNull>
			<isNotNull prepend="AND" property="notStatus">
				STATUS != #notStatus#
			</isNotNull>
			<isNotNull prepend="AND" property="statusList">
				<iterate prepend="" property="statusList" open=" (" close=") " conjunction="OR">
				STATUS = #statusList[]#
				</iterate>
			</isNotNull>
			<isNotNull prepend="AND" property="name">
				NAME = #name#
			</isNotNull>
			<isNotNull prepend="AND" property="money">
				MONEY = #money#
			</isNotNull>
			<isNotNull prepend="AND" property="moneyLE">
				<![CDATA[
				MONEY <= #moneyLE#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="moneyGE">
				<![CDATA[
				MONEY >= #moneyGE#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="nameLike">
				NAME LIKE #nameLike#
			</isNotNull>
			<isNotNull prepend="AND" property="isChecked">
				ISCHECKED = #isChecked#
			</isNotNull>
			<isNotNull prepend="AND" property="isDeleted">
				ISDELETED = #isDeleted#
			</isNotNull>
			<isNotNull prepend="AND" property="isRemoved">
				ISREMOVED = #isRemoved#
			</isNotNull>
			<isNotNull prepend="AND" property="fromReqDate">
				<![CDATA[
				REQ_DATE >= #fromReqDate#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="toReqDate">
				<![CDATA[
				REQ_DATE <= #toReqDate#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="fromProcDate">
				<![CDATA[
				PROC_DATE >= #fromProcDate#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="toProcDate">
				<![CDATA[
				PROC_DATE <= #toProcDate#
				]]>
			</isNotNull>
		ORDER BY ID DESC
		<isNotNull property="fromRow">
		LIMIT #fromRow#, #rowSize#
		</isNotNull>
	</select>
	
	<select id="selectMoneyInCount" resultClass="integer" parameterClass="map">
		SELECT COUNT(*) FROM tbl_money_out
		WHERE TYPE = 'in'
			<isNotNull prepend="AND" property="userId">
				USERID = #userId#
			</isNotNull>
			<isNotNull prepend="AND" property="userIdLike">
				USERID LIKE #userIdLike#
			</isNotNull>
			<isNotNull prepend="AND" property="nickNameLike">
				NICKNAME LIKE #nickNameLike#
			</isNotNull>
			<isNotNull prepend="AND" property="status">
				STATUS = #status#
			</isNotNull>
			<isNotNull prepend="AND" property="notStatus">
				STATUS != #notStatus#
			</isNotNull>
			<isNotNull prepend="AND" property="statusList">
				<iterate prepend="" property="statusList" open=" (" close=") " conjunction="OR">
				STATUS = #statusList[]#
				</iterate>
			</isNotNull>
			<isNotNull prepend="AND" property="name">
				NAME = #name#
			</isNotNull>
			<isNotNull prepend="AND" property="money">
				MONEY = #money#
			</isNotNull>
			<isNotNull prepend="AND" property="nameLike">
				NAME LIKE #nameLike#
			</isNotNull>
			<isNotNull prepend="AND" property="isChecked">
				ISCHECKED = #isChecked#
			</isNotNull>
			<isNotNull prepend="AND" property="isDeleted">
				ISDELETED = #isDeleted#
			</isNotNull>
			<isNotNull prepend="AND" property="isRemoved">
				ISREMOVED = #isRemoved#
			</isNotNull>
			<isNotNull prepend="AND" property="fromReqDate">
				<![CDATA[
				REQ_DATE >= #fromReqDate#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="toReqDate">
				<![CDATA[
				REQ_DATE <= #toReqDate#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="fromProcDate">
				<![CDATA[
				PROC_DATE >= #fromProcDate#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="toProcDate">
				<![CDATA[
				PROC_DATE <= #toProcDate#
				]]>
			</isNotNull>
	</select>
	
	<select id="selectMoneyInSum" resultClass="long" parameterClass="map">
		SELECT SUM(MONEY) FROM tbl_money_out
		WHERE TYPE = 'in'
			<isNotNull prepend="AND" property="userId">
				USERID = #userId#
			</isNotNull>
			<isNotNull prepend="AND" property="userIdLike">
				USERID LIKE #userIdLike#
			</isNotNull>
			<isNotNull prepend="AND" property="nickNameLike">
				NICKNAME LIKE #nickNameLike#
			</isNotNull>
			<isNotNull prepend="AND" property="status">
				STATUS = #status#
			</isNotNull>
			<isNotNull prepend="AND" property="notStatus">
				STATUS != #notStatus#
			</isNotNull>
			<isNotNull prepend="AND" property="statusList">
				<iterate prepend="" property="statusList" open=" (" close=") " conjunction="OR">
				STATUS = #statusList[]#
				</iterate>
			</isNotNull>
			<isNotNull prepend="AND" property="name">
				NAME = #name#
			</isNotNull>
			<isNotNull prepend="AND" property="money">
				MONEY = #money#
			</isNotNull>
			<isNotNull prepend="AND" property="nameLike">
				NAME LIKE #nameLike#
			</isNotNull>
			<isNotNull prepend="AND" property="isChecked">
				ISCHECKED = #isChecked#
			</isNotNull>
			<isNotNull prepend="AND" property="isDeleted">
				ISDELETED = #isDeleted#
			</isNotNull>
			<isNotNull prepend="AND" property="isRemoved">
				ISREMOVED = #isRemoved#
			</isNotNull>
			<isNotNull prepend="AND" property="fromReqDate">
				<![CDATA[
				REQ_DATE >= #fromReqDate#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="toReqDate">
				<![CDATA[
				REQ_DATE <= #toReqDate#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="fromProcDate">
				<![CDATA[
				PROC_DATE >= #fromProcDate#
				]]>
			</isNotNull>
			<isNotNull prepend="AND" property="toProcDate">
				<![CDATA[
				PROC_DATE <= #toProcDate#
				]]>
			</isNotNull>
	</select>
	
	<select id="selectMoneyInWaitCount" resultClass="integer">
		SELECT COUNT(*) FROM tbl_money_out t
		WHERE TYPE = 'in' AND STATUS = 'MC001' AND ISDELETED='N'
	</select>
</sqlMap>